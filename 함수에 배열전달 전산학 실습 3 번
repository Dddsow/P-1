#include<stdio.h>

int main(){

  getchar();
  return 0;
}

input(int data[],int size)
{
  int cnt_i;
  printf("%d개의 데이터를 입력하세요 : \n",size);
  
  for(cnt_i=0;cnt_i<size;cnt_i++){
    scanf_s("%d",&data[cnt_i]);
  }
  

}

getTotal(int data[],int size)
{
  int cnt_i;
  int total=0;
  for(cnt_i=0;cnt_i<size;cnt_i++)
  {
    total += data[cnt_i];        
  }
  
  return total;
}
